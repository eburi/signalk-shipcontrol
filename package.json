{
  "name": "signalk-shipcontrol",
  "version": "0.2.3",
  "description": "Get data like tank readings from Ship Control. If you have Ship Control on your boat, you can use this plugin to read data displayed in Ship Control",
  "main": "plugin/index.js",
  "keywords": [
    "signalk-node-server-plugin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/panaaj/signalk-flags"
  },
  "author": "eburi",
  "license": "Apache-20",
  "scripts": {
    "build": "tsc",
    "build-declaration": "tsc --declaration --allowJs false",
    "watch": "npm run build -- -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build -- -w",
    "format": "prettier --write src/*"
  },
  "dependencies": {
    "@signalk/server-api": "^2.7.1",
    "baconjs": "^1.0.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/baconjs": "^0.7.28",
    "@types/express": "^5.0.3",
    "@types/node": "^24.1.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "express": "^5.1.0",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5"
  },
  "signalk-plugin-enabled-by-default": true
}
